    <!-- variable declaration -->
    <% let add = 'Delhi' %> 
    <% let resident = 'Delhi resident' %> 
    <% let out = 'outside Delhi' %> 

    <!-- if else -->
    <% if(add === 'Delhi'){ %> 
       <h4> <%= resident %> </h4>
    <% } else { %> 
        <%= out %>
    <% } %> 


    <!-- function -->
    <% function calculateAdd(a, b){ %> 
        <% return a+b %> 
    <% } %> 

    <h3><%= calculateAdd(10,20) %> </h3> 

    <!-- for loop -->

    <% if(data.length){ %> 
        <% data.forEach((el) => { %>
        <h3> <%= el.userId %> </h3> 
        <h3> <%= el.title %> </h3> 
        <% }) %> 
    <% }else { %> 
        <h3><%= error %></h3>
    <% } %> 





